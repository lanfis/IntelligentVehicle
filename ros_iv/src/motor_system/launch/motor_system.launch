<launch>
  <node name="motor_system" pkg="motor_system" type="motor_system" respawn="false" output="screen">
    <remap from="motor_system/motor_cmd" to="/ros_arduino_msgs"/>
    <remap from="motor_system/motor_status" to="/arduino_ros_msgs"/>
  </node>
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM1"/>
    <param name="baud" value="115200"/>
  </node>
</launch>
